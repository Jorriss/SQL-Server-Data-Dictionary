  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@table_name">
            <Value>=Parameters!table_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> SELECT      SysInd.object_id As Table_Object_ID
            ,SysInd.name AS Index_Name
            ,SysInd.index_id AS Index_ID
            ,SysInd.type_desc AS Index_Type
            ,SysInd.is_disabled 
            ,SysInd.is_unique
            ,SysInd.is_primary_key
            ,SysInd.is_unique_constraint
            ,SysFil.name AS FileGroup
 FROM       sys.indexes SysInd  WITH (NOLOCK)
 LEFT JOIN  sys.filegroups SysFil  WITH (NOLOCK) ON SysInd.data_space_id = SysFil.data_space_id
 JOIN       sys.tables  SysTbl  WITH (NOLOCK) ON SysInd.object_id = SysTbl.object_id
 WHERE      SysTbl.name = @table_name</CommandText>
      </Query>
