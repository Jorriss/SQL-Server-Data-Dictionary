  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@table_name">
            <Value>=Parameters!table_name.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@index_id">
            <Value>=Parameters!index_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
 SELECT     SysInd.object_id As Table_Object_ID
            ,SysCol.name
            ,SysIndCol.index_column_id
            ,SysIndCol.key_ordinal
            ,SysIndCol.partition_ordinal
            ,SysIndCol.is_descending_key
            ,SysIndCol.is_included_column            
 FROM       sys.index_columns SysIndCol  WITH (NOLOCK)
 INNER JOIN sys.indexes SysInd  WITH (NOLOCK) ON SysIndCol.object_id = SysInd.object_id
                               AND SysInd.index_id = SysIndCol.index_id
 INNER JOIN sys.columns SysCol  WITH (NOLOCK) ON SysIndCol.column_id = SysCol.column_id
                               AND SysCol.object_id = SysInd.object_id
 JOIN       sys.tables  SysTbl  WITH (NOLOCK) ON SysInd.object_id = SysTbl.object_id
 WHERE      SysTbl.name = @table_name
 AND           SysInd.index_id = @index_id</CommandText>
      </Query>
      <Fields>
        <Field Name="Table_Object_ID">
          <DataField>Table_Object_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="index_column_id">
          <DataField>index_column_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="key_ordinal">
          <DataField>key_ordinal</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="partition_ordinal">
          <DataField>partition_ordinal</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="is_descending_key">
          <DataField>is_descending_key</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="is_included_column">
          <DataField>is_included_column</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
